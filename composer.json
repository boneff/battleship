{
    "name": "root/battleship",
    "description": "A basic battleships game",
    "type": "project",
    "authors": [
        {
            "name": "boneff",
            "email": "boneff.i@gmail.com"
        }
    ],
    "require": {
        "ocramius/proxy-manager": "^2.1",
        "nikic/php-parser": "^4.0",
        "friendsofphp/php-cs-fixer": "^2.10"
    },
    "require-dev": {
        "phpro/grumphp": "^0.14.0",
        "phpunit/phpunit": "^6.5",
        "squizlabs/php_codesniffer": "*",
        "jakub-onderka/php-parallel-lint": "^1.0"
    },
    "autoload": {
        "psr-4": {"Battleships\\": "src/"}
    },
    "extra": {
        "grumphp": {
            "config-default-path": "grumphp.yml"
        }
    }
}
